local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/Documantation12/Universal-Vehicle-Script/main/Library.lua"))()local b=nil;repeat task.wait()until game:IsLoaded()repeat task.wait()until game:GetService("Players").LocalPlayer;local c=game:GetService("Players")local d=game:GetService("RunService")local e=game:GetService("UserInputService")local c=c.LocalPlayer;pcall(function()b=a.new("Custom Vehicle Script - Made by Genesis_ui",5013109572)end)if not b then warn("UI failed to load. Retrying...")repeat task.wait(1)pcall(function()b=a.new("Custom Vehicle Script - Made by Genesis_ui",5013109572)end)until b end;if b.GUI then b.GUI.Draggable=true;b.GUI.Active=true;b.GUI.Selectable=true end;local a={Background=Color3.fromRGB(30,30,50),Glow=Color3.fromRGB(90,90,255),Accent=Color3.fromRGB(70,70,140),LightContrast=Color3.fromRGB(50,50,80),DarkContrast=Color3.fromRGB(25,25,40),TextColor=Color3.fromRGB(255,255,255)}for a,c in pairs(a)do pcall(function()b:setTheme(a,c)end)end;local function a(a)if not a then return nil end;return a:FindFirstAncestor(c.Name.."'s Car")or a:FindFirstAncestorWhichIsA("Model")end;local function f(b)local c=c.Character;if not c then return end;local a=a(c:FindFirstChildWhichIsA("Humanoid")and c:FindFirstChildWhichIsA("Humanoid").SeatPart)if not a then return end;a:SetPrimaryPartCFrame(b)end;local b=b:addPage("Vehicle",8356815386)local b=b:addSection("Flight, Speed & Brakes")local g,h,i=false,false,true;local j,k,l=1,0.025,0.15;b:addToggle("Enable Flight",false,function(a)g=a end)b:addSlider("Flight Speed",100,0,800,function(a)j=a/100 end)b:addToggle("Enable Speed Boost",false,function(a)h=a end)b:addSlider("Speed Multiplier",25,0,100,function(a)k=a/1000 end)b:addToggle("Enable Brakes",true,function(a)i=a end)b:addSlider("Brake Strength",15,0,50,function(a)l=a/100 end)d.RenderStepped:Connect(function(b)local c=c.Character;if not c then return end;local c=c:FindFirstChildWhichIsA("Humanoid")if not c then return end;local c=c.SeatPart;if not c or not c:IsA("VehicleSeat")then return end;local a=a(c)if not a then return end;if not a.PrimaryPart then local b=a:FindFirstChildWhichIsA("BasePart")if b then a.PrimaryPart=b else return end end;if g then local c=a:GetPrimaryPartCFrame()local d=Vector3.new(0,0,0)if e:IsKeyDown(Enum.KeyCode.W)then d=d+workspace.CurrentCamera.CFrame.LookVector*j end;if e:IsKeyDown(Enum.KeyCode.S)then d=d-workspace.CurrentCamera.CFrame.LookVector*j end;if e:IsKeyDown(Enum.KeyCode.A)then d=d-workspace.CurrentCamera.CFrame.RightVector*j end;if e:IsKeyDown(Enum.KeyCode.D)then d=d+workspace.CurrentCamera.CFrame.RightVector*j end;if e:IsKeyDown(Enum.KeyCode.E)then d=d+Vector3.new(0,j,0)end;if e:IsKeyDown(Enum.KeyCode.Q)then d=d-Vector3.new(0,j,0)end;for a,a in pairs(a:GetDescendants())do if a:IsA("BasePart")then a.AssemblyLinearVelocity=Vector3.new(0,0,0)a.AssemblyAngularVelocity=Vector3.new(0,0,0)a.Velocity=Vector3.new(0,0,0)end end;f(c+d*b*60)end;if h and e:IsKeyDown(Enum.KeyCode.W)then c.AssemblyLinearVelocity*=Vector3.new(1+k,1,1+k)end;if i and e:IsKeyDown(Enum.KeyCode.S)then c.AssemblyLinearVelocity*=Vector3.new(1-l,1,1-l)end end)print("Genesis UI Script Loaded Successfully!")
